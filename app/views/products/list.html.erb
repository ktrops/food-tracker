<h1> Enter your items</h1>

<%= form_for @product, url: list_path, method: :post do %>
<div id="listForm">
  <label class="labelItem" for="product_name0">Name</label>
  <input type="text" name="products[][name]" class="inputItem" id="product_name0">
  <input type="button" name="add_another" id="addAnother" value="Add another item">
  <input type="submit" name="commit" value="Import items">
</div>
<% end %>
